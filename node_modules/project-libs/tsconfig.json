{
  "compilerOptions": {
    "outDir": "./build/",
    "noImplicitAny": true,
    // "module": "es6",
    "allowJs": true,
    "target": "es5",
    "lib":["es5", "dom", "dom.iterable", "es2015", "es2016", "es2017", "scripthost"],
    "downlevelIteration": true,
    "emitBOM": true,
    "jsx": "react"
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}